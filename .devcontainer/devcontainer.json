{
  "build": {
    "dockerfile": "Dockerfile"
  },
  "name": "VueNET - vue-net",
  "customizations": {
    "vscode": {
      "extensions": [
        "Tyriar.sort-lines",
        "Vue.volar",
        "Vue.vscode-typescript-vue-plugin",
        "antfu.iconify",
        "antfu.unocss",
        "antfu.vite",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "formulahendry.auto-complete-tag",
        "lokalise.i18n-ally",
        "ms-azuretools.vscode-docker",
        "ms-playwright.playwright",
        "redhat.vscode-xml"
      ]
    }
  },
  "forwardPorts": [
    3000
  ],
  "postAttachCommand": "cd vue-net && npm run dev"
}